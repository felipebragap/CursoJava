import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
         Scanner sc = new Scanner(System.in);

        int lineNumber = 0;
        while (sc.hasNext()) {
            lineNumber++;
            String line = sc.nextLine();
            System.out.println(lineNumber + " " + line);
        }

        sc.close();
    } 
}
